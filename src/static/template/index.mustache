{{>header}}
    <div class="row-fluid">
        {{#3-Column-Row}}
        <div class="span4">
            <h2><i class="{{Icon}}"></i>&nbsp;{{Title}}</h2>
            <ul class="icons-ul">
            {{#List}}
                <li><i class="icon-li icon-ok"></i>{{Item}}</li>
            {{/List}}
            </ul>
            <p>{{{Description}}}</p>
        </div>
        {{/3-Column-Row}}
    </div>

    <div class="row-fluid marketing">
        {{#2-Column-Row}}
        <div class="span6">
            <h2><i class="{{Icon}}"></i>&nbsp;{{Title}}</h2>
            <ul class="icons-ul">
            {{#List}}
                <li><i class="icon-li icon-ok"></i>{{Item}}</li>
            {{/List}}
            </ul>
            <p>{{{Description}}}</p>
        </div>
        {{/2-Column-Row}}
    </div>
{{>footer}}